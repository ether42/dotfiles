[alias]
  st = status
  ci = commit
  # %C(auto) may not work on older git version
  lg = log --graph --date=short --pretty=tformat:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%an, %ad)%Creset'
  ft = fetch --all --tags --prune
[core]
  pager = less -R
#[merge]
# conflictStyle = diff3
[push]
  default = simple
[color]
  ui = auto
